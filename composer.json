{
    "name": "amoori/licensing-server",
    "description": "License server for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "license",
        "license-server",
        "license-manager"
    ],
    "authors": [
        {
            "name": "EgoistDeveloper",
            "email": "epsilonelittr@gmail.com"
        },
        {
            "name": "AmirMehrabian",
            "email": "aut0run2011@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/AmirMehrabi/licensing-server/issues",
        "source": "https://github.com/AmirMehrabi/licensing-server"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "illuminate/support": "^5.8",
        "jeremykendall/php-domain-parser": "^5.7.0",
        "amoori/ultimate-support": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^7.42",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "LaravelReady\\LicenseServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelReady\\LicenseServer\\LicenseServerServiceProvider"
            ],
            "aliases": {
                "LicenseServer": "LaravelReady\\LicenseServer\\Facades\\LicenseServer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
